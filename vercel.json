{
    "builds": [
        {
            "src": "/api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ],
    "env": {
        "GITHUB_TOKEN": "@github-token",
        "GEMINI_API_KEY": "@GEMINI_API_KEY"
    }
}